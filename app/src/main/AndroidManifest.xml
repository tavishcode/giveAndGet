<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.giveandget.giveandget">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:theme="@style/AppThemeNoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EventsActivity" android:label="Volunteer Events"></activity>
        <activity android:name=".EventDetails" android:label="Event Details"></activity>
        <activity android:name=".VolunteerForm" android:label="Volunteering Form"></activity>
        <activity android:name=".RedeemActivity" android:label="Redeem Voucher"></activity>
    </application>

</manifest>
